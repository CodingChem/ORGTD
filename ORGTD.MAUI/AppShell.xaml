<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ORGTD.MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ORGTD.MAUI"
    Shell.FlyoutBehavior="Flyout"
    Title="ORGTD.MAUI">
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Journal"
        Content="This is a Journal"
        />
    <ShellContent
        Title="Tasks"
        Content="Some Tasks"
        />
    <ShellContent
        Title="Agenda"
        Content="Your Agenda"
        />

</Shell>
